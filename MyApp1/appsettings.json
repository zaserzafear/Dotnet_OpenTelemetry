{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://aspire-dashboard:18889",
  "OTEL_SERVICE_NAME": "MyApp1",
  "MyApp2": {
    "BaseUrl": "http://myapp2:8080",
    "Timeout": 30
  },
  "RabbitMQ": {
    "Host": "rabbitmq://rabbitmq:5672/",
    "User": "guest",
    "Pass": "guest",
    "AuthRequest": {
      "PrefetchCount": 16,
      "ConcurrentMessageLimit": 200
    }
  },
  "FeatureConfiguration": {
    "Consumers": {
      "Auth": false
    },
    "Publishers": {
      "Auth": true
    }
  }
}
